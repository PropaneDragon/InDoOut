<interfaceElements:CommonBlockDisplay x:Class="InDoOut_Viewer.UI.Controls.BlockView.BlockView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InDoOut_Viewer.UI.Controls.BlockView"
             xmlns:entities="clr-namespace:InDoOut_UI_Common.Controls.CoreEntityRepresentation;assembly=InDoOutUICommon"
             xmlns:interfaceElements="clr-namespace:InDoOut_UI_Common.InterfaceElements;assembly=InDoOutUICommon"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="CommonBlockDisplay_Loaded" Unloaded="CommonBlockDisplay_Unloaded">
    <Border BorderThickness="1" BorderBrush="#02000000">
        <Grid>
            <ScrollViewer x:Name="Scroll_Content" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden">
                <Viewbox x:Name="Viewbox_Content">
                    <Canvas x:Name="Canvas_Content" Width="10000" Height="10000">
                        <entities:UIFunction Canvas.Left="4900" Canvas.Top="4900"/>
                    </Canvas>
                </Viewbox>
            </ScrollViewer>
        </Grid>
    </Border>
</interfaceElements:CommonBlockDisplay>
