  <ItemGroup>
    <Page Update="Controls\Core\TitleBar.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Style\TitleBarStyle.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
