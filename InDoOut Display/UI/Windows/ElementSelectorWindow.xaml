<Window x:Class="InDoOut_Display.UI.Windows.ElementSelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InDoOut_Display.UI.Windows"
        xmlns:core="clr-namespace:InDoOut_UI_Common.Controls.Core;assembly=InDoOutUICommon"
        xmlns:elementSelector="clr-namespace:InDoOut_Display.UI.Controls.ElementSelector"
        mc:Ignorable="d"
        Title="Elements" Height="600" Width="1000" WindowStartupLocation="CenterOwner">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <elementSelector:ElementSelector x:Name="ElementSelector_Main" Margin="0,80,0,0" />
        </ScrollViewer>
        <core:TitleBar VerticalAlignment="Top"/>
    </Grid>
</Window>
